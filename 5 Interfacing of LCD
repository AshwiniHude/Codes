#include<P18f4520.h>
#include<stdio.h>

#pragma config OSC = HS	// High-speed oscillator 
#pragma config WDT = OFF		// Watchdog Timer disabled
#pragma config LVP = OFF	// Low-voltage Programming disabled

unsigned char arr1[] = "TE E&TC"; 
unsigned char arr2[] = "R.H.Sapat CoE";


void main(void)
{  
    LCDinit() ;
    DisplayRow(0,arr1);
    DisplayRow(1,arr2);
    while(1);
}
